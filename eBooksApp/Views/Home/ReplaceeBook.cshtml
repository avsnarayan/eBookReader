
@{
    ViewData["Title"] = "UploadeBook";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Upload eBook</h1>
<div class="form-group">
    <form name="Upload" method="post" asp-action="ReplaceeBook" asp-route-id="@ViewBag.eBookId" enctype="multipart/form-data">

        
        <div class="form-group">
            
            <input name="MYeBook" type="file" />
        </div>
        
        <div class="form-group">
            <input type="submit" value="Upload" class="btn btn-primary"/>
        </div>
        
    </form>
</div>
